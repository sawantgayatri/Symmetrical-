#include <stdio.h>
int main() {
  int n, reversed = 0, remainder, org;
    printf("Enter an integer: ");
    scanf("%d", &n);
    org = n;

    while (n != 0) {
        remainder = n % 10;
        reversed = reversed * 10 + remainder;
        n /= 10;
    }
    if (org == reversed)
        printf("%d is a palindrome.", org);
    else
        printf("%d is not a palindrome.", org);
    return 0;

}
