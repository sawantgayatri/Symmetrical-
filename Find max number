#include<stdio.h>
int main()
{
	int num1,num2,max;
	printf("enter number::");
	scanf("%d %d\n",num1,num2);
	printf("num1=%d,num2=%d\n",num1,num2);
	if(num1 > num2)
	{
		max = num1;
		printf("%d is max number\n");	
	}
	else
	{
		max = num2;
		printf("%d is max number\n");
	}
	printf("the maximum valu is",max);
    return 0;

}
